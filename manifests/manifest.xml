<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>58490919-4e8d-46e0-96f6-4c88b3e9745d</Id>
  <Version>1.1.0.0</Version>
  <ProviderName>Willem Sleegers</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="tidystats"/>
  <Description DefaultValue="A Word add-in to reproducibly report statistics."/>
  <IconUrl DefaultValue="https://localhost:3000/images/tidystats-icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://localhost:3000/images/tidystats-icon-64.png"/>
  <SupportUrl DefaultValue="https://www.tidystats.io/support"/>
  <AppDomains>
    <AppDomain>https://www.tidystats.io</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Document"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://localhost:3000/index.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Document">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
            <ExtensionPoint xsi:type="PrimaryCommandSurface">
              <OfficeTab id="TabInsert">
                <Group id="Group">
                  <Label resid="Group.Label"/>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="64" resid="Icon.64x64"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                    <bt:Image size="128" resid="Icon.128x128"/>
                  </Icon>
                  <Control xsi:type="Button" id="TaskpaneButton">
                    <Label resid="TaskpaneButton.Label"/>
                    <Supertip>
                      <Title resid="TaskpaneButton.Label"/>
                      <Description resid="TaskpaneButton.Tooltip"/>
                    </Supertip>
                    <Icon>
                      <bt:Image size="16" resid="Icon.16x16"/>
                      <bt:Image size="32" resid="Icon.32x32"/>
                      <bt:Image size="64" resid="Icon.64x64"/>
                      <bt:Image size="80" resid="Icon.80x80"/>
                      <bt:Image size="128" resid="Icon.128x128"/>
                    </Icon>
                    <Action xsi:type="ShowTaskpane">
                      <TaskpaneId>ButtonId1</TaskpaneId>
                      <SourceLocation resid="Taskpane.Url"/>
                    </Action>
                  </Control>
                </Group>
              </OfficeTab>
            </ExtensionPoint>
          </DesktopFormFactor>
        </Host>
      </Hosts>
      <Resources>
        <bt:Images>
          <bt:Image id="Icon.16x16" DefaultValue="https://localhost:3000/images/statistics-icon-16.png"/>
          <bt:Image id="Icon.32x32" DefaultValue="https://localhost:3000/images/statistics-icon-32.png"/>
          <bt:Image id="Icon.64x64" DefaultValue="https://localhost:3000/images/statistics-icon-64.png"/>
          <bt:Image id="Icon.80x80" DefaultValue="https://localhost:3000/images/statistics-icon-80.png"/>
          <bt:Image id="Icon.128x128" DefaultValue="https://localhost:3000/images/statistics-icon-128.png"/>
        </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://www.tidystats.io"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://localhost:3000/index.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="Get started with tidystats!"/>
        <bt:String id="Group.Label" DefaultValue="Statistics"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="Insert Statistics"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="tidystats loaded succesfully. Go to the Insert tab and click the 'Insert Statistics' button to get started."/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Click to open tidystats"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>